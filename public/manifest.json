{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "vite.svg"
  },
  "permissions": [
    "tabs",
    "alarms",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  }
}